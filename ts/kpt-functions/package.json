{
  "name": "kpt-functions",
  "version": "0.14.5",
  "description": "KPT functions framework library",
  "author": "KPT Authors",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/GoogleContainerTools/kpt-functions-sdk.git",
    "directory": "ts/kpt-functions"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "!**/*_test.*"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -Rf dist/ node_modules/",
    "lint": "eslint -c .eslintrc.js --ext .ts src/ && prettier \"src/**\" \"*.json\" --check",
    "lint-license": "license-checker --onlyAllow 'Apache-2.0;MIT;BSD;BSD-2-Clause;BSD-3-Clause;ISC;CC-BY-3.0;CC0-1.0;Unlicense'",
    "format": "eslint -c .eslintrc.js --ext .ts src/ --fix && prettier \"src/**\" \"*.json\" --write",
    "pretest": "npm run build",
    "test": "jasmine --config=jasmine.json",
    "posttest": "npm run lint",
    "gen-docs": "typedoc src"
  },
  "dependencies": {
    "argparse": "^1.0.10",
    "js-yaml": "^3.14.0",
    "rw": "^1.3.3"
  },
  "devDependencies": {
    "@types/argparse": "^1.0.36",
    "@types/jasmine": "^3.5.14",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash": "^4.14.161",
    "@types/node": "^12.12.55",
    "@types/rw": "file:@types/rw",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.5.1",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-react": "^7.20.6",
    "jasmine": "^3.6.1",
    "license-checker": "^25.0.1",
    "lodash": "^4.17.20",
    "prettier": "2.1.2",
    "tslint": "^6.1.3",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.3"
  }
}
