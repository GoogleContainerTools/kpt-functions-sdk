{
  "name": "example-package",
  "version": "0.0.1",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -Rf node_modules/ dist/",
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "kpt": "node node_modules/@googlecontainertools/create-kpt-functions/dist/cli.js",
    "kpt:type-create": "npm run kpt type-create",
    "kpt:function-create": "npm run kpt function-create",
    "kpt:docker-create": "npm run kpt docker-create",
    "kpt:docker-build": "npm test && npm run kpt docker-build",
    "kpt:docker-push": "npm run kpt docker-push",
    "kpt:docker-publish": "npm run kpt docker-build && npm run kpt docker-push",
    "kpt:workflow-create": "npm run kpt workflow-create"
  },
  "dependencies": {
    "@googlecontainertools/kpt-functions": "^0.1.0"
  },
  "devDependencies": {
    "@googlecontainertools/create-kpt-functions": "^0.3.0",
    "@types/jasmine": "^3.3.12",
    "@types/node": "^12.7.2",
    "jasmine": "^3.4.0",
    "tslint": "^5.19.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.5.3"
  },
  "dev_kpt": {
    "docker_repo_base": "gcr.io/kpt-functions-demo"
  }
}
