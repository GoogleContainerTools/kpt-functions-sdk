{
  "name": "@googlecontainertools/create-kpt-functions",
  "version": "0.7.0",
  "description": "KPT functions CLI",
  "author": "KPT Authors",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:GoogleContainerTools/kpt-functions-sdk.git",
    "directory": "ts/create-kpt-functions"
  },
  "bin": {
    "create-kpt-functions": "dist/cli.js"
  },
  "files": [
    "bin/typegen",
    "dist/",
    "!**/*_test.*",
    "templates/"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "watch": "tsc --watch",
    "prepublishOnly": "npm test && npm run lint",
    "clean": "rm -Rf node_modules/ dist/",
    "lint": "tslint -p package.json; ts-node node_modules/prettier/bin-prettier.js \"src/**\" \"*.json\" --check",
    "lint-license": "jsgl --local .",
    "format": "ts-node node_modules/prettier/bin-prettier.js \"src/**\" \"*.json\" --write",
    "pretest": "npm run build",
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "posttest": "npm run lint",
    "depcheck": "ts-node node_modules/npm-check/bin/cli.js --ignore jasmine"
  },
  "dependencies": {
    "@kubernetes/client-node": "^0.10.2",
    "argparse": "^1.0.10",
    "chalk": "^2.4.2",
    "cli-interact": "^0.1.9",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.4",
    "is-valid-npm-name": "0.0.4",
    "mustache": "^3.0.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@types/argparse": "^1.0.36",
    "@types/cli-interact": "^0.1.0",
    "@types/fs-extra": "^8.0.0",
    "@types/jasmine": "^3.3.12",
    "@types/glob": "^7.1.1",
    "@types/mustache": "^0.8.32",
    "@types/request-promise": "^4.1.44",
    "@types/validator": "^10.11.3",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^12.7.2",
    "jasmine": "^3.4.0",
    "js-green-licenses": "^1.1.0",
    "npm-check": "^5.9.0",
    "prettier": "1.18.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "typescript": "^3.5.3"
  },
  "prettier": {
    "arrowParens": "always",
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
